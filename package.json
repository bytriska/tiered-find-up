{
  "name": "@bytriska/priority-find-up",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@10.27.0",
  "description": "Find files by walking up parent directories and resolving them based on an ordered priority manifest.",
  "author": "Triska Mahfud K (https://github.com/bytriska)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/bytriska",
  "homepage": "https://github.com/bytriska/priority-find-up#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytriska/priority-find-up.git"
  },
  "bugs": "https://github.com/bytriska/priority-find-up/issues",
  "keywords": [
    "find-up",
    "priority-search",
    "tiered-search",
    "filesystem",
    "config-resolver",
    "typescript",
    "nodejs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublish": "pnpm build",
    "release": "pnpm test && bumpp",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "lint-staged": "^16.2.7",
    "prettier": "3.3.3",
    "simple-git-hooks": "^2.13.1",
    "tsup": "^8.5.1",
    "typescript": "<5.9.0",
    "vitest": "^4.0.16"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm test && pnpm lint:fix"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,json,yaml,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
